local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ItzzAvi535/Lume-Library-V2/refs/heads/main/Library", true))()

local window = library:CreateWindow("Lume library V2")

local mainTab = window:AddTab("Main")

mainTab:AddLabel("Put your text here")

mainTab:AddButton("Button name here", function()
	print("Farming started")
end)

mainTab:AddToggle("Auto Farm", function(state)
    if state then
        print("Auto-farming enabled!")
        -- Add your auto-farming code here
    else
        print("Auto-farming disabled!")
        -- Add your code 
    end
end, false)  -- false means the toggle starts disabled
